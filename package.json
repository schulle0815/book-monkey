{
  "name": "book-monkey",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "ng serve",
    "watch": "npm run build:dev -- --watch --poll 1000",
    "build": "npm run build:dev",
    "build:base": "ng build",
    "build:prod": "npm run clean && npm run build:base -- --configuration=production",
    "build:dev": "npm run build:base -- --configuration=development",
    "test": "ng test",
    "lint": "ng lint",
    "source-map-explorer": "npm run build:prod -- --source-map && source-map-explorer dist/main*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.12",
    "@angular/cdk": "^12.2.12",
    "@angular/common": "~12.2.12",
    "@angular/compiler": "~12.2.12",
    "@angular/core": "~12.2.12",
    "@angular/forms": "~12.2.12",
    "@angular/material": "^12.2.12",
    "@angular/platform-browser": "~12.2.12",
    "@angular/platform-browser-dynamic": "~12.2.12",
    "@angular/router": "~12.2.12",
    "rxjs": "^7.5.2",
    "semantic-ui-css": "^2.4.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.16",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~3.8.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "source-map-explorer": "^2.5.0",
    "typescript": "~4.3.5"
  }
}
